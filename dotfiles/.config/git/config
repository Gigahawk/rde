[user]
	name = Andrew Tropin
	email = andrew@trop.in
	signingkey = ABEDF9050C0AF46E

[alias]
	aliases = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /' | sort    # list all aliases
	st = status

[commit]
	gpgsign = true

[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
